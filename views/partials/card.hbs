<div class="card">
    <div class="center">
        <img style="height: 100px; width: auto" class="img-fluid" src="{{this.image_url}}" alt="Card image cap">
    </div>
    <div class="card-body">
        <h5 class="card-title">{{this.name}}</h5>
        <p class="card-text">{{this.description}}</p>
        <a href="" class="btn btn-primary">Link</a>
    </div>
</div>